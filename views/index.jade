extends layout

block content
    #container
        #menu
            #video-item.menu-item
            #photo-item.menu-item
            #music-item.menu-item
            #browser-item.menu-item
            #settings-item.menu-item
        #content
            #home-content.content-item
                include content/home.jade
            #video-content.content-item.hidden
                include content/videos.jade
            #photo-content.content-item.hidden
                include content/photos.jade
            #music-content.content-item.hidden
                include content/music.jade
            #browser-content.content-item.hidden
                include content/browser.jade
            #settings-content.content-item.hidden
                include content/settings.jade
